package com.util;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.PreparedStatement;

public class UtilMethods {
	String url = "jdbc:oracle:thin:@localhost:1521/orcl.fios-router.home";
	String username = "c##goutham";
	String password = "Oracle_2020";

	public Connection establishConnection(String command) {
		try {
			Class.forName("oracle.jdbc.driver.OracleDriver");
			Connection con=DriverManager.getConnection(url,username,password);
			return con.prepareStatement(command);
			
		} catch (Exception e) {
			e.printStackTrace();
			return null;
		}
		
		
	}
}
